  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gish/friskis-js-api-wrapper/issues"
  },
  "homepage": "https://github.com/gish/friskis-js-api-wrapper#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "dotenv": "^2.0.0",
    "expect": "^1.20.2",
    "mocha": "^2.5.3",
    "moment": "^2.14.1"
  },
  "dependencies": {
    "request": "^2.74.0",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0"
  },
  "standard": {
